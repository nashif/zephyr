    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Saniycheck fast",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/scripts/sanitycheck",
            "console": "integratedTerminal",
            "env": {
                "ZEPHYR_BASE": "${workspaceRoot}",
                "ZEPHYR_TOOLCHAIN_VARIANT": "gnuarmemb",
                "ZEPHYR_SDK_INSTALL_DIRX": "/home/nashif/Work/SDK/0.10.1",
                "GNUARMEMB_TOOLCHAIN_PATH": "/Users/anashif/zephyr/SDK/gcc-arm-none-eabi-7-2018-q2-update"
            },
            "args": [
                "-p",
                "mps2_an385",
                "-p",
                "frdm_k64f",
                "-T",
                "samples/synchronization",
                "-vv",
                //"--report-dir",
                //"my_sanity",
                //"--report-name",
                //"overhaul"
            ]
        },
