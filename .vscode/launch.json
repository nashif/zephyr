    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Saniycheck qemu_x86",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/scripts/sanitycheck",
            "console": "integratedTerminal",
            "env": {
                "ZEPHYR_BASE": "${workspaceRoot}",
                "ZEPHYR_TOOLCHAIN_VARIANT": "zephyr",
                "ZEPHYR_SDK_INSTALL_DIR": "/home/nashif/Work/SDK/0.10.1"
            },
            "args": [    
                //"-p",
                //"qemu_x86",       
                //"-p",
                //"frdm_k64f",                   
                "-T",
                "tests/kernel",
                "-v"
            ]
        },
        {
            "name": "Python: Saniycheck native_posix",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/scripts/sanitycheck",
            "console": "integratedTerminal",
            "env": {
                "ZEPHYR_BASE": "${workspaceRoot}",
                "ZEPHYR_TOOLCHAIN_VARIANT": "zephyr",
                "ZEPHYR_SDK_INSTALL_DIR": "/home/nashif/Work/SDK/0.10.1"
            },
            "args": [
                "-p",
                "native_posix",
                "-T",
                "tests/kernel",
                "-vv"
            ]
        }
    ]
}